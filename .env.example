# Supabase Configuration
# Copy this file to .env and fill in your actual values

# Supabase Project URL
SUPABASE_URL=https://your-project-id.supabase.co

# Supabase Service Role Key (for RPC functions)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Supabase Anonymous Key (for public access)
SUPABASE_ANON_KEY=your-anon-key-here

# Database Connection (optional, for direct psql access)
DATABASE_URL=postgresql://postgres:[password]@db.your-project-id.supabase.co:5432/postgres

# n8n Configuration (for data processing workflows)
N8N_ENCRYPTION_KEY=your-n8n-encryption-key
N8N_HOST=localhost
N8N_PORT=5678

# Development Environment
NODE_ENV=development